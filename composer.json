{
    "name": "mrhdolek/simple-redis-model",
    "description": "Simple model to redis",
    "keywords": [
        "MrHDOLEK",
        "laravel",
        "simple-redis-model"
    ],
    "homepage": "https://github.com/MrHDOLEK/simple-redis-model",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksander Kowalski",
            "email": "mrhdolek14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "predis/predis": "^2.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "ext-redis": "*"
    },
    "require-dev": {
        "blumilksoftware/codestyle": "*",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "MrHDOLEK\\SimpleRedisModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "cs:fix": "./vendor/bin/php-cs-fixer fix --diff --config codestyle.php",
        "cs:check": "./vendor/bin/php-cs-fixer fix --dry-run --diff --config codestyle.php",
        "phpstan": [
            "php -d memory_limit=600M vendor/bin/phpstan analyse -c phpstan.neon"
        ],
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedisModelServiceProvider"
            ],
            "aliases": {
                "Skeleton": "MrHDOLEK\\SimpleRedisModel\\Facades\\Skeleton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
